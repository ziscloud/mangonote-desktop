    "css-loader": "^0.  23.1",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "^1.0.1",
    "electron-reload": "^1.0.2",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "uglify-js": "^2.7.0",
    "url-loader": "^0.5.7",
    "file-loader": "^0.9.0",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
